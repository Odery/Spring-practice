<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="notes" class="inversionOfControl.TeacherBook"/>
    <bean id="mathTeacher" class="inversionOfControl.MathTeacher">
        <property name="notes" ref="notes"/>
    </bean>
    <bean id="musicTeacher" class="inversionOfControl.MusicTeacher">
        <constructor-arg ref="notes"/>
    </bean>

</beans>